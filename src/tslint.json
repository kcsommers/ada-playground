{
  "extends": "tslint:recommended",
  "rules": {
    "object-literal-key-quotes": false,
    "ordered-imports": false,
    "trailing-comma": false,
    "no-empty": false,
    "object-literal-sort-keys": false,
    "member-access": false,
    "quotemark": [
      false
    ],
    "max-line-length": {
      "options": [
        120
      ]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": {
      "severity": "warning",
      "options": [
        "debug",
        "info",
        "log",
        "time",
        "timeEnd",
        "trace"
      ]
    }
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        120
      ]
    }
  }
}